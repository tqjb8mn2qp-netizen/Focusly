<svg width="512" height="512" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" rx="110" fill="url(#grad1)"/>
  
  <!-- Book icon -->
  <g transform="translate(256, 256)">
    <!-- Book -->
    <rect x="-120" y="-80" width="240" height="160" rx="12" fill="#fff" opacity="0.95"/>
    <rect x="-120" y="-80" width="240" height="20" rx="12" fill="#4A90E2"/>
    
    <!-- Pages -->
    <line x1="-10" y1="-60" x2="-10" y2="60" stroke="#4A90E2" stroke-width="3"/>
    <line x1="10" y1="-60" x2="10" y2="60" stroke="#4A90E2" stroke-width="3"/>
    
    <!-- Timer circle -->
    <circle cx="70" cy="-50" r="35" fill="#F5A623" opacity="0.95"/>
    <circle cx="70" cy="-50" r="28" fill="#fff"/>
    <line x1="70" y1="-50" x2="70" y2="-70" stroke="#F5A623" stroke-width="4" stroke-linecap="round"/>
    <line x1="70" y1="-50" x2="85" y2="-50" stroke="#F5A623" stroke-width="4" stroke-linecap="round"/>
    
    <!-- Checkmark -->
    <circle cx="-70" cy="50" r="25" fill="#7ED321" opacity="0.95"/>
    <polyline points="-82,50 -72,60 -58,40" fill="none" stroke="#fff" stroke-width="5" stroke-linecap="round" stroke-linejoin="round"/>
  </g>
</svg>
